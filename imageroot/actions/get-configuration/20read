# ... existing imports and setup ...

# Prepare return variable
config = {}

# Existing config population...
config["host"] = os.getenv("TRAEFIK_HOST", "")
config["configured"] = True if config["host"] else False
config["http_port"] = os.getenv("TRAEFIK_HTTP_PORT", "80")
config["http2https"] = os.getenv("TRAEFIK_HTTP2HTTPS") == "True"
config["lets_encrypt"] = os.getenv("TRAEFIK_LETS_ENCRYPT") == "True"

# Add sample_apps_enabled (default to False if not set)
config["sample_apps_enabled"] = os.getenv("SAMPLE_APPS_ENABLED", "False") == "True"

# LDAP domain config...
lp = Ldapproxy()
domains = [{"name": key, "label": key, "value": key} for key in lp.get_domains_list()]
config['ldap_domain_list'] = domains
config['ldap_domain'] = os.environ.get("LDAP_DOMAIN", '')

# Service checks remain unchanged...
# (lemonldapng and lemonldapng-app logic)

# Dump the configuration to stdout
json.dump(config, fp=sys.stdout)

